config {
  type: "incremental"
}

js {
  const sql = require("@dataform/sql");
}

${ when(incremental(), "select * from (") }

select ${sql().timestamps.currentUTC()} as ts

${ when(incremental(), `) as subquery where ts > (select max(ts) from ${self()}) or (select max(ts) from ${self()}) is null`) }
